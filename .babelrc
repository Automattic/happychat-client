{
	"presets": [ [ "env" ] ],
	"plugins": [
		"syntax-jsx",
		"transform-class-properties",
		"transform-object-rest-spread",
		"transform-react-display-name",
		"transform-react-jsx",
		"transform-export-extensions",
		[ "module-resolver", { "root": ["./"] } ],
		[ "inline-json", { "matchPattern": "(development|production)" } ]
	]
}
